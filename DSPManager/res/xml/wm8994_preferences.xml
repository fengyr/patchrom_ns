<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen android:title="@string/dsp_settings_wm8994"
  xmlns:android="http://schemas.android.com/apk/res/android">
    <PreferenceCategory android:title="@string/category_wm8994_internal_speaker" android:key="wm8994_internal_speaker_category">
